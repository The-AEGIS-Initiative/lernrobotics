{
  "name": "game-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/dark-theme": "^2.0.1",
    "@material-ui/core": "^4.9.5",
    "@react-pdf/renderer": "^1.6.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "ace-builds": "^1.4.8",
    "antd": "^4.0.2",
    "aws-amplify": "^3.0.8",
    "axios": "^0.19.1",
    "babel-plugin-import": "^1.13.0",
    "better-docs": "^1.4.7",
    "console-feed": "^2.8.11",
    "customize-cra": "^0.9.1",
    "dotenv": "^8.2.0",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "load-script": "^1.0.0",
    "markdown-it": "^10.0.0",
    "markdown-it-mathjax": "^2.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-ace": "^8.0.0",
    "react-app-rewire-less": "^2.1.3",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.12.0",
    "react-game-kit": "^1.0.6",
    "react-hook-form": "^5.1.1",
    "react-hotkeys-hook": "^1.6.0",
    "react-latex": "^1.6.0",
    "react-markdown": "^4.3.1",
    "react-modal": "^3.11.2",
    "react-pdf": "^4.1.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-splitter-layout": "^4.0.0",
    "react-test-renderer": "^16.13.1",
    "react-unity-webgl": "^7.1.10",
    "socket.io-client": "^2.3.0"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "start:headless": "BROWSER=none react-app-rewired start",
    "build": "react-app-rewired build",
    "build:dev": "REACT_APP_BUILD_ENV=dev REACT_APP_BACKEND_URL=$BACKEND_URL react-app-rewired build",
    "build:prod": "REACT_APP_BUILD_ENV=prod REACT_APP_BACKEND_URL=$BACKEND_URL react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "test:report": "npm run test -- --coverage --watchAll=false",
    "e2e:local": "cypress run --env baseUrl=http://localhost:3000/",
    "e2e:dev": "cypress run --env baseUrl=https://development-robobot.aegisinitiative.io/",
    "cypress:local": "run-s start e2e:local",
    "cypress:dev": "run-s start e2e:dev",
    "docs": "jsdoc -c jsdoc.conf.json",
    "lint": "eslint src/**/*.js --fix",
    "commit": "commit-wizard"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "cypress": "^4.4.0",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest-sonar-reporter": "^2.0.0",
    "jsdoc": "^3.6.3",
    "lint-staged": "^10.1.7",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.5",
    "react-app-rewired": "^2.1.5",
    "start-server-and-test": "^1.11.0",
    "wait-on": "^4.0.2"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm start && npm run-script e2e:local",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,md}": "prettier --write"
  },
  "jest": {
    "testResultsProcessor": "jest-sonar-reporter"
  }
}
